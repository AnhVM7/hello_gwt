<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel>
		<g:Label text="Full Name:" />
		<g:TextBox ui:field="fullNameTextBox" />

		<g:Label text="Email:" />
		<g:TextBox ui:field="emailTextBox" />

		<g:Label text="Age:" />
		<g:TextBox ui:field="ageTextBox" />

		<g:Label text="Address:" />
		<g:TextBox ui:field="addressTextBox" />

		<g:Label text="Gender:" />
		<g:ListBox ui:field="genderListBox" />

		<g:Button ui:field="saveButton" text="Save" />
	</g:VerticalPanel>
</ui:UiBinder>